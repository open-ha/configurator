{
    "minimum-stability": "stable",
    "config": {
        "sort-packages": true
    },
    "require": {
        "symfony/console": "^6.1",
        "symfony/dependency-injection": "^6.1"
    },
    "require-dev": {
        "mikey179/vfsstream": "^1.6",
        "phpunit/php-code-coverage": "^9.2",
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.7",
        "vimeo/psalm": "^4.26"
    },
    "autoload": {
        "psr-4": {
            "OpenHa\\Configurator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OpenHa\\Configurator\\Test\\Unit\\": "tests/unit/",
            "OpenHa\\Configurator\\Test\\Integration\\": "tests/integration/"
        }
    },
    "bin": ["bin/openha-configurator"]
}
